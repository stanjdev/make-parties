<!-- events-index -->
<h1>Events</h1>
<div class="row mt-1 col-lg-6" >
  {{#each events}}
  <div class="card">
    <img class="img-fluid" src="{{this.imgUrl}}" alt="Card image cap">
    <h2>{{this.title}}</h2>
    <p>{{this.desc}}</p>
    <div>
      <button type="button" class="btn btn-info">RSVP</button>
      <button type="button" class="btn btn-primary">View</button>
    </div>
  </div>
  {{/each}}
</div>
